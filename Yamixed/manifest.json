{
  "name": "Yamixed",
  "version": "0.0.1",
  "browser_action":
  {
    "default_icon": "icon.png",
    "default_popup" : "html/popup.html"
  },
  "permissions": ["tabs"],
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery/jquery-1.11.1.min.js", "js/content.js"]
    }
  ]
}